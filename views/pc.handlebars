<div>
    <p class="list-container">Character List</p>
</div>

{{#each characters as |char| }}
    {{#unless npc}}
    <div class="char-card">
        <div class="subrace-button">Name: {{char.name}} | Race: {{char.race}} <br>
            <div>Class: {{char.characterclass}} | Background: {{char.background}}</div>
        </div>
    </div>
    {{/unless}}
{{/each}}